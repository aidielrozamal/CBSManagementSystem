    <div class="container">
        <div class="row">
            <div class="col-12">
                <nav class="main-nav">
                   <!-- ***** Logo Start ***** -->
                            <a href="index.html" class="logo">
                                <img src="/assets/images/logocuervos.png" alt="Cuervos Logo" class="cute-logo">
                            </a>
                        <!-- ***** Logo End ***** -->
                    <!-- ***** Search End ***** -->
                    <div class="search-input">
                      <form id="search" action="#">
                        <input type="text" placeholder="Type Something" id='searchText' name="searchKeyword" onkeypress="handle" />
